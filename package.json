{
	"name": "obp-svelte-components",
	"version": "0.0.1",
  "description": "A reusable component library for the Open Bank Project",
	"author": {
    "name": "Open Bank Project",
    "url": "https://www.openbankproject.com/"
  },
  "scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.49.1",
		"@skeletonlabs/skeleton": "^4.0.0",
		"@skeletonlabs/skeleton-svelte": "^4.0.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/markdown-it": "^14.1.2",
		"@types/prismjs": "^1.26.5",
		"dotenv": "^17.2.1",
		"jsdom": "^26.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"uuid": "^11.1.0",
		"vite": "^6.2.6",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@langchain/core": "^0.3.57",
		"@lucide/svelte": "^0.513.0",
		"arctic": "^3.7.0",
		"bits-ui": "^2.8.10",
		"ioredis": "^5.6.1",
		"jwt-decode": "^4.0.0",
		"langchain": "^0.3.27",
		"markdown-it": "^14.1.0",
		"prismjs": "^1.30.0",
		"svelte-kit-connect-redis": "^0.1.0",
		"svelte-kit-sessions": "^0.4.0"
	},
	"keywords": [
		"svelte",
    "components",
    "openbankproject"
	]
}
